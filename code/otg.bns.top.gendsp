, 			{
				"box" : 				{
					"code" : "//highpass + scaling on inputs \r\nfilterinputs(mic1,mic2,f,g)\r\n{\r\n\treturn hpf2(mic1,f,0.707) * g, \r\n\t\t   hpf2(mic2,f,0.707) * g; \t\r\n}\r\n\r\n//Settable params \r\nParam controlGain(1, min=0.1,max=100); \r\nParam samplerGain(10, min=0.1, max=100); \r\nParam grainsGain(1, min=0.1, max=100); \r\nParam distance(1,min=0, max=20); //nb maybe this max is too restrictive?  \r\nParam threshold(0.75,min=0, max=1); \r\nParam speakerPairs(3, min=1,max=4); \r\nHistory inAmp0(1); \r\nHistory lag(0); \r\nHistory trig(0); \r\nHistory outAmpGrain(0); \r\n\r\n//20\" ramp for filling buffers with poke\r\nindex = counter(1,0,20*samplerate); \r\n\r\n//HPF and scale input depending on values of lag, inAmp0 \r\nhighpass_f = 100 + (lag * 100); \r\nmic1, mic2 = filterinputs(in1,in2,highpass_f,inAmp0); \r\n\r\n//update core control signals \r\ninAmp0, sw, lag, trig, thr = controls((mic1 + mic2), threshold=threshold);\r\n\r\n//big delay line for input audio, gated by sw\r\ndelay_in1 = gatedelay(mic1, sw); \r\ndelay_in2 = gatedelay(mic2, sw); \r\n\r\n//extra control signals using delayed sig\r\ninAmp2 = envelope(controlGain * (delay_in1 + delay_in2),delaytime=20); \r\ninAmp1 = 1 - inAmp2; \r\n\r\n//write to sampler buffer\r\npoke(memory1, inAmp2 * (mic1 + mic2), index); \r\n\r\n//trigger sampler plyabck via sw, with set delay + lag-based playback rate\r\nev = change(sw) == 1; \r\nsample1 = sampler(ev,lag, mult=10.1, delaytime=6666); \r\nsample2 = sampler(ev,lag, mult=10.0, delaytime=13333); \r\nsample3 = sampler(ev,lag, mult=4.99, delaytime=10000); \r\n\r\n//write high passed sampler playback to grain buffer\r\nsamps_hp = hpf2((sample1 + sample2 + sample3) * samplerGain,100 + (lag * 1000),0.7); \r\npoke(memory2,samps_hp, index);\r\n\r\n//proceed through grain buffer based on lag\r\ngrainPointer = phasor(1/(lag*40)); \r\n//grain playback \r\ng1, g2 = grains(1-inAmp0, grainPointer,inAmp2,inAmp2); \r\ng1 *= (0.5 * ((1-outAmpGrain)*(2*inAmp0))); \r\ng2 *= (0.5 * ((1-outAmpGrain)*(2*inAmp0))); \r\noutAmpGrain = envelope(controlGain * (g1 + g2),delaytime=1); \r\n\r\n//output routing / delays \r\nsig1 = delay_in1 * inAmp1; \r\nsig2 = delay_in2 * inAmp1; \r\n\r\nsig3 = sample1 * samplerGain * inAmp1; \r\nsig4 = sample2 * samplerGain * inAmp1; \r\nsig5 = sample3 * samplerGain * inAmp1; \r\nsig6 = g1; \r\nsig7 = g2;\r\n\r\nout1, out2, out3, out4, out5, out6, out7, out8 = outputs(sig1,sig2,sig3,sig4,sig5,sig6,sig7, lag, speakerpairs=speakerPairs, distance=distance); \r\n\r\nout9 = inAmp0; \r\nout10 = thr; ",
